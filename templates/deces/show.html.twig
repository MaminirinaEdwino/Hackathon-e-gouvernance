{% extends 'base.html.twig' %}

{% block title %}Deces{% endblock %}

{% block body %}
    <h1>Deces</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ dece.id }}</td>
            </tr>
            <tr>
                <th>Nom</th>
                <td>{{ dece.nom }}</td>
            </tr>
            <tr>
                <th>Prenom</th>
                <td>{{ dece.prenom }}</td>
            </tr>
            <tr>
                <th>Date_dece</th>
                <td>{{ dece.dateDece ? dece.dateDece|date('Y-m-d') : '' }}</td>
            </tr>
            <tr>
                <th>Cin</th>
                <td>{{ dece.cin }}</td>
            </tr>
            <tr>
                <th>Fichier_retour</th>
                <td>{{ dece.fichierRetour }}</td>
            </tr>
            <tr>
                <th>DateDemande</th>
                <td>{{ dece.dateDemande ? dece.dateDemande|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>DateRetour</th>
                <td>{{ dece.dateRetour ? dece.dateRetour|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>Confirmation</th>
                <td>{{ dece.confirmation ? 'Yes' : 'No' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_deces_index') }}">back to list</a>

    <a href="{{ path('app_deces_edit', {'id': dece.id}) }}">edit</a>

    {{ include('deces/_delete_form.html.twig') }}
{% endblock %}
